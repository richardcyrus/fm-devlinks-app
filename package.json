{
  "name": "fm-devlinks-app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "fix": "concurrently --raw npm:fix:*",
    "fix:code": "eslint --cache --cache-location ./node_modules/.cache/eslint --fix --ignore-path .gitignore \"**/*.[jt]s?(x)\"",
    "fix:css": "stylelint \"**/*.css\" --cache --cache-location ./node_modules/.cache/stylelint --fix --allow-empty-input",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{css,js,jsx,cjs,mjs,ts,tsx,md,mdx,astro,json}\"",
    "lint": "concurrently --raw npm:lint:*",
    "lint:code": "eslint --cache --cache-location ./node_modules/.cache/eslint --ignore-path .gitignore \"**/*.[jt]s?(x)\"",
    "lint:css": "stylelint \"**/*.css\" --cache --cache-location ./node_modules/.cache/stylelint --ignore-path .gitignore --allow-empty-input",
    "prepare": "husky install",
    "start": "remix-serve ./build/server/index.js",
    "test:unit": "vitest --run",
    "test:unit:coverage": "vitest run --coverage",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@remix-run/node": "^2.10.0",
    "@remix-run/react": "^2.10.0",
    "@remix-run/serve": "^2.10.0",
    "class-variance-authority": "^0.7.0",
    "isbot": "^5.1.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.10.0",
    "@remix-run/eslint-config": "^2.10.0",
    "@remix-run/testing": "^2.10.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@svgr/rollup": "^8.1.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@typescript-eslint/typescript-estree": "^7.15.0",
    "@vitest/coverage-v8": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "axe-core": "^4.9.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-testing-library": "^6.2.2",
    "happy-dom": "^14.12.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "micromatch": "^4.0.7",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^12.1.5",
    "postcss-preset-env": "^9.5.15",
    "postcss-sort-media-queries": "^5.2.0",
    "prettier": "^3.3.2",
    "stylelint": "^16.6.1",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-order": "^6.0.4",
    "typescript": "^5.5.2",
    "unplugin-fonts": "^1.1.1",
    "vite": "^5.3.2",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "browserslist": {
    "production": [
      ">= 0.5%",
      "last 2 major versions",
      "not dead",
      "Chrome >= 64",
      "Edge >= 79",
      "Firefox >= 67",
      "Firefox ESR",
      "opera >= 51",
      "iOS >= 12",
      "Safari >= 12",
      "not Explorer <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 edge version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "axe-core",
    "css-custom-properties",
    "frontend-mentor",
    "html5",
    "reactjs",
    "remix-run",
    "stylelint",
    "testing-library",
    "typescript",
    "vitest"
  ],
  "volta": {
    "node": "20.15.0"
  }
}
